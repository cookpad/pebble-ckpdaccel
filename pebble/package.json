{
  "name": "ckpdaccel",
  "author": "David Stosik",
  "version": "1.0.0",
  "keywords": [
    "pebble-app"
  ],
  "private": true,
  "dependencies": {
    "pebble-clay": "^1.0.4",
    "pebblejs": "davidstosik/pebblejs#devel-fix-accel-data-time"
  },
  "pebble": {
    "displayName": "Cookpad Accelerometer",
    "uuid": "fb733e99-ea89-44ed-81c3-3b5f6e2aa9ca",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "aplite",
      "basalt",
      "chalk",
      "diorite"
    ],
    "capabilities": [
      "configurable"
    ],
    "watchapp": {
      "watchface": false
    },
    "messageKeys": [
      "endpoint",
      "identifier",
      "accelerometer_rate",
      "accelerometer_samples",
      "debug"

        {
          "file": "images/music_icon_play.png",
          "name": "IMAGE_MUSIC_ICON_PLAY",
          "type": "png"
        },
        {
          "file": "images/music_icon_pause.png",
          "name": "IMAGE_MUSIC_ICON_PAUSE",
          "type": "png"
        },
        {
          "file": "images/floppy.png",
          "name": "IMAGE_FLOPPY",
          "type": "png"
        }
      ]
    }
  }
}
